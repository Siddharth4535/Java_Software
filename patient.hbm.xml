<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package = "medicare">
    <class name = "Patient" table = "patientTable">
        <id name = "id" column = "id"/>
        <property name = "name" column = "name"/> <!-- column can be given according to the user can be same as name or you can change your accordingly -->
        <property name = "contact" column = "contact"/>
        <property name = "age" column = "age"/>
        <property name = "gender" column = "gender"/>
        <property name = "address" column = "address"/>
        <property name = "city" column = "city"/>
        <property name = "floor" column = "floor"/>
        <property name = "roomNo" column = "roomNo"/>
    </class>
</hibernate-mapping>