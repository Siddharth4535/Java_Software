<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package = "medicare">
    <class name = "Checkup" table = "checkupTable">
        <id name = "ck_id" column = "checkupId"/>
        <id name = "id" column = "patientId"/>
        <id name = "doc_id" column = "doctorId"/>
        <id name = "med_id" column = "mediclaimId"/>
        <id name = "eqp_id" column = "equipmentId"/>
        <property name = "name" column = "name"/>
        <property name = "model" column = "model"/>
        <property name = "cost" column = "cost"/>
    </class>
</hibernate-mapping>