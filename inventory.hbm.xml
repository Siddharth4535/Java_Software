<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package = "medicare">
    <class name = "Inventory" table = "inventoryTable">
        <id name = "prod_id" column = "id"/>
        <property name = "name" column = "name"/>
        <property name = "description" column = "description"/>
        <property name = "expDate" column = "expiryDate"/>
        <property name = "qtyOrder" column = "quantityOrdered"/>
        <property name = "qtyRecivied" column = "quantityRecivied"/>
        <property name = "cost" column = "cost"/>
        <property name = "stock" column = "stock"/>
    </class>
</hibernate-mapping>