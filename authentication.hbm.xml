<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package = "medicare">
    <class name = "Authentication" table = "authenticationTable">
        <id name = "id" column = "id"/>
        <property name = "password" column = "password"/> <!-- column can be given according to the user can be same as name or you can change your accordingly -->
        <property name = "post" column = "post"/>
    </class>
</hibernate-mapping>