<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package = "medicare">
    <class name = "Doctor" table = "doctorTable">
        <id name = "doc_id" column = "id"/>
        <property name = "name" column = "name"/>
        <property name = "contact" column = "contact"/>
        <property name = "gender" column = "gender"/>
        <property name = "speciality" column = "speciality"/>
        <property name = "experience" column = "experience"/>
    </class>
</hibernate-mapping>